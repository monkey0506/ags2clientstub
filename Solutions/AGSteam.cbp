				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-lsteam_api" />
					<Add option="-fPIC" />
					<Add option="-m32" />
					<Add directory="../../../Steamworks/131/sdk/redistributable_bin/linux32" />
				</Linker>
			</Target>
		</Build>

				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-fPIC" />
					<Add option="-Wl,rpath,." />
					<Add option="-DLINUX_VERSION" />
